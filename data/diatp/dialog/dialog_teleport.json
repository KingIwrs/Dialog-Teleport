{
    "type": "minecraft:multi_action",
    "title": {
        "translate": "dialogs.dialog_teleport.title",
        "fallback": "Dialog Teleport"
    },
    "external_title": {
        "translate": "dialogs.dialog_teleport.external_title",
        "fallback": "Dialog Teleport"
    },
    "actions": [
        {
            "label": {
                "text": "Edit Entries",
                "color": "yellow"
            },
            "width": 100,
            "action": {
                "type": "run_command",
                "command": "trigger diatp.edit_entries"
            }
        },
        {
            "label": {
                "text": "Remove Entries",
                "color": "red"
            },
            "width": 100,
            "action": {
                "type": "run_command",
                "command": "trigger diatp.remove_entry"
            }
        },
        {
            "label": {
                "text": "Teleport Menu",
                "color": "green"
            },
            "width": 200,
            "action": {
                "type": "run_command",
                "command": "trigger diatp.teleport_menu"
            }
        }
    ],
    "exit_action": {
        "label": {
            "translate": "dialogs.dialog_teleport.back",
            "fallback": "Back"
        }
    }
}
